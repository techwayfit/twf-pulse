{
  "name": "Change Management Workshop",
  "description": "Assess readiness and address concerns",
  "category": "Retrospective",
  "iconEmoji": "üîÑ",
  "config": {
    "title": "Change Management Session",
    "goal": "Assess organizational readiness and plan change adoption",
    "context": "Preparing the organization for upcoming changes",
    "settings": {
      "durationMinutes": 90,
      "allowAnonymous": true,
      "allowLateJoin": false,
      "showResultsDuringActivity": true
    },
    "joinFormSchema": {
      "fields": [
        {
          "name": "name",
          "label": "Your Name",
          "type": "text",
          "required": true
        },
        {
          "name": "department",
          "label": "Department",
          "type": "text",
          "required": true
        },
        {
          "name": "role",
          "label": "Role",
          "type": "select",
          "required": false,
          "options": [
            "Leadership",
            "Manager",
            "Individual Contributor",
            "Support Staff"
          ]
        }
      ]
    },
    "activities": [
      {
        "order": 1,
        "type": "Poll",
        "title": "Change Sentiment",
        "prompt": "How do you feel about the upcoming changes?",
        "durationMinutes": 5,
        "config": {
          "options": [
            {"id": "very-excited", "label": "üòä Very excited", "description": null},
            {"id": "optimistic", "label": "üôÇ Optimistic", "description": null},
            {"id": "neutral", "label": "üòê Neutral", "description": null},
            {"id": "concerned", "label": "üòü Concerned", "description": null},
            {"id": "very-worried", "label": "üò® Very worried", "description": null}
          ],
          "allowMultiple": false,
          "minSelections": 1,
          "maxSelections": 1,
          "allowCustomOption": false,
          "customOptionPlaceholder": "Other (please specify)",
          "randomizeOrder": false,
          "showResultsAfterSubmit": false,
          "maxResponsesPerParticipant": 1
        }
      },
      {
        "order": 2,
        "type": "WordCloud",
        "title": "Concerns & Hopes",
        "prompt": "What concerns or hopes do you have about the change?",
        "durationMinutes": 5
      },
      {
        "order": 3,
        "type": "Quadrant",
        "title": "Impact vs Readiness Analysis",
        "prompt": "Map areas affected by impact and readiness",
        "durationMinutes": 15,
        "config": {
          "xAxisLabel": "Current Readiness",
          "yAxisLabel": "Change Impact",
          "topLeftLabel": "High Impact, Low Readiness (Needs Focus)",
          "topRightLabel": "High Impact, High Readiness (Champions)",
          "bottomLeftLabel": "Low Impact, Low Readiness (Monitor)",
          "bottomRightLabel": "Low Impact, High Readiness (Quick Wins)"
        }
      },
      {
        "order": 4,
        "type": "GeneralFeedback",
        "title": "Support Needed",
        "prompt": "What support do you need to successfully adapt?",
        "durationMinutes": 10,
        "config": {
          "categoriesEnabled": true,
          "categories": [
            {"id": "training", "label": "Training", "icon": "üìö"},
            {"id": "resources", "label": "Resources", "icon": "üõ†Ô∏è"},
            {"id": "communication", "label": "Communication", "icon": "üí¨"},
            {"id": "time", "label": "Time", "icon": "‚è∞"},
            {"id": "leadership-support", "label": "Leadership Support", "icon": "üëî"}
          ],
          "requireCategory": true,
          "showCharacterCount": true,
          "maxLength": 1000,
          "minLength": 10,
          "placeholder": "Describe the support you need...",
          "allowAnonymous": true,
          "maxResponsesPerParticipant": 5
        }
      },
      {
        "order": 5,
        "type": "Poll",
        "title": "Biggest Barrier",
        "prompt": "What's the biggest barrier to successful change?",
        "durationMinutes": 5,
        "config": {
          "options": [
            {"id": "lack-clarity", "label": "Lack of clarity", "description": null},
            {"id": "insufficient-training", "label": "Insufficient training", "description": null},
            {"id": "time-constraints", "label": "Time constraints", "description": null},
            {"id": "team-resistance", "label": "Resistance from team", "description": null},
            {"id": "technical-challenges", "label": "Technical challenges", "description": null}
          ],
          "allowMultiple": false,
          "minSelections": 1,
          "maxSelections": 1,
          "allowCustomOption": true,
          "customOptionPlaceholder": "Other barrier (please specify)",
          "randomizeOrder": false,
          "showResultsAfterSubmit": true,
          "maxResponsesPerParticipant": 1
        }
      }
    ]
  }
}
