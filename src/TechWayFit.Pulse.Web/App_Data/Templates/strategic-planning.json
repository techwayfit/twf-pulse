{
  "name": "Strategic Planning",
  "description": "Executive strategy session with prioritization",
  "category": "ProductDiscovery",
  "iconEmoji": "üéØ",
  "config": {
    "title": "Strategic Planning Workshop",
    "goal": "Align on strategic priorities and action plans",
    "context": "Executive team strategy session",
    "settings": {
      "durationMinutes": 180,
      "allowAnonymous": false,
      "allowLateJoin": false,
      "showResultsDuringActivity": true
    },
    "joinFormSchema": {
      "fields": [
        {
          "name": "name",
          "label": "Your Name",
          "type": "text",
          "required": true
        },
        {
          "name": "role",
          "label": "Role",
          "type": "select",
          "required": true,
          "options": [
            "CEO/President",
            "VP/Director",
            "Manager",
            "Team Lead",
            "Contributor"
          ]
        }
      ]
    },
    "activities": [
      {
        "order": 1,
        "type": "WordCloud",
        "title": "Vision Keywords",
        "prompt": "What words best describe our vision for the next year?",
        "durationMinutes": 5
      },
      {
        "order": 2,
        "type": "GeneralFeedback",
        "title": "Strategic Initiatives",
        "prompt": "What strategic initiatives should we pursue?",
        "durationMinutes": 10,
        "config": {
          "categoriesEnabled": true,
          "categories": [
            {"id": "growth", "label": "Growth", "icon": "üìà"},
            {"id": "innovation", "label": "Innovation", "icon": "üí°"},
            {"id": "efficiency", "label": "Efficiency", "icon": "‚ö°"},
            {"id": "market-expansion", "label": "Market Expansion", "icon": "üåç"},
            {"id": "culture", "label": "Culture", "icon": "ü§ù"}
          ],
          "requireCategory": true,
          "showCharacterCount": true,
          "maxLength": 1000,
          "minLength": 10,
          "placeholder": "Describe the strategic initiative...",
          "allowAnonymous": false,
          "maxResponsesPerParticipant": 5
        }
      },
      {
        "order": 3,
        "type": "Quadrant",
        "title": "Initiative Prioritization",
        "prompt": "Map initiatives by impact and effort",
        "durationMinutes": 15,
        "config": {
          "xAxisLabel": "Effort Required",
          "yAxisLabel": "Strategic Impact",
          "topLeftLabel": "High Impact, Low Effort (Quick Wins)",
          "topRightLabel": "High Impact, High Effort (Long-term Bets)",
          "bottomLeftLabel": "Low Impact, Low Effort (Nice to Have)",
          "bottomRightLabel": "Low Impact, High Effort (Avoid)"
        }
      },
      {
        "order": 4,
        "type": "Poll",
        "title": "Top Priority Vote",
        "prompt": "Which strategic direction should we prioritize?",
        "durationMinutes": 5,
        "config": {
          "options": [
            {"id": "market-expansion", "label": "Market expansion", "description": null},
            {"id": "product-innovation", "label": "Product innovation", "description": null},
            {"id": "operational-excellence", "label": "Operational excellence", "description": null},
            {"id": "talent-development", "label": "Talent development", "description": null},
            {"id": "customer-experience", "label": "Customer experience", "description": null}
          ],
          "allowMultiple": false,
          "minSelections": 1,
          "maxSelections": 1,
          "allowCustomOption": true,
          "customOptionPlaceholder": "Other priority (please specify)",
          "randomizeOrder": false,
          "showResultsAfterSubmit": true,
          "maxResponsesPerParticipant": 1
        }
      },
      {
        "order": 5,
        "type": "GeneralFeedback",
        "title": "Action Plans",
        "prompt": "What are the concrete next steps for each priority?",
        "durationMinutes": 10,
        "config": {
          "categoriesEnabled": false,
          "showCharacterCount": true,
          "maxLength": 1000,
          "minLength": 10,
          "placeholder": "Describe the action plan...",
          "allowAnonymous": false,
          "maxResponsesPerParticipant": 5
        }
      }
    ]
  }
}
